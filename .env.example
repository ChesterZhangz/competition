# MongoDB Atlas
MONGODB_URI=mongodb+srv://<user>:<password>@cluster.mongodb.net/math-competition

# Redis (Docker)
REDIS_URL=redis://localhost:6379

# JWT Secrets (generate with: openssl rand -base64 64)
JWT_ACCESS_SECRET=your-access-secret-min-64-chars-long-generate-with-openssl
JWT_REFRESH_SECRET=your-refresh-secret-min-64-chars-long-generate-with-openssl
JWT_ACCESS_EXPIRES=15m
JWT_REFRESH_EXPIRES=7d

# Server
PORT=3001
NODE_ENV=development
FRONTEND_URL=http://localhost:5173

# Email Service (SMTP)
EMAIL_HOST=smtp.example.com
EMAIL_PORT=465
EMAIL_SECURE=true
EMAIL_USER=your-email@example.com
EMAIL_PASS=your-email-password

# AI API (DeepSeek)
DEEPSEEK_API_KEY=your-deepseek-api-key
DEEPSEEK_API_URL=https://api.deepseek.com
